@{
    ViewData["Title"] = "Teste da API";
}
<h2>@ViewData["Title"]</h2>

<div class="row">
    <div class="col-md-3">
        <p>Resultado da consulta à API</p>
        <table class="table table-bordered table-hover">
            @foreach (var item in ViewBag.Result)
            {
                <tr><td class="text-success">@item</td></tr>
            }
        </table>
    </div>
</div>